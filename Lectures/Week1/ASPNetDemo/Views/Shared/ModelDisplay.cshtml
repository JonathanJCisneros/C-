<!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Model Display</title>
    </head>
    <body>
        <h1>@ViewBag.MyModel.FavoritePokemon</h1>

        <table>
            <thead>
                <tr>
                    <th>My Numbers</th>
                </tr>
            </thead>
            <tbody>
                @foreach (int number in ViewBag.MyModel.NumbersILike)
                    {
                        <tr>
                            <td>@number</td>
                        </tr>
                    }
            </tbody>
        </table>
    </body>
</html>